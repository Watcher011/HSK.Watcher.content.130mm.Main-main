				<wickTicks>
					<min>5</min>
					<max>10</max>
				</wickTicks>
			</li>
			
          <li Class="CompProperties_Rottable">
        <daysToRotStart>1.5</daysToRotStart>
        <rotDestroys>true</rotDestroys>
         </li>
   
